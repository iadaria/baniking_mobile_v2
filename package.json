{
  "name": "baniking_mobile_v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android-shake": "adb shell input keyevent 82",
    "android-baniking": "NODE_ENV=product npx react-native run-android",
    "ios-baniking": "NODE_ENV=product npx react-native run-ios",
    "ios-se": "react-native run-ios --simulator=\"iPhone 6\"",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn start --reset-cache",
    "reset-cache": "yarn cache clean --force && react-native start --reset-cache",
    "android-clean": "cd android && ./gradlew cleanBuildCache",
    "ios-reset-cache": "pod cache clean --all && pod deintergrate && pod clean && xcodebuild clean",
    "clean-build": "rm -rf ios/build android/app/build",
    "reset": "react-native run-android -- --reset-cache",
    "eject": "react-native eject",
    "clean-project": "npx react-native-clean-project",
    "pod-update": "yarn upgrade --latest && npx pod-install",
    "pod": "npx pod-install",
    "other-clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && watchman watch-del-all",
    "remove-android-build": "rm -rf android/build",
    "brew": "brew update && brew upgrade",
    "android-bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle-android-debug": "cd android && ./gradlew bundleDebug && cd ..",
    "bundle-android-release": "cd android && ./gradlew bundleRelease && cd ..",
    "android-build": "cd android && ./gradlew build && cd ..",
    "build-android-debug": "cd android && ./gradlew assembleDebug && cd ../",
    "build-android-release": "cd android && ./gradlew assembleRelease && cd ../",
    "android-debug": "npx react-native run-android --variant=debug",
    "android-release": "npx react-native run-android --variant=release",
    "android-uninstall": "adb uninstall \"com.baniking_mobile\"",
    "ios-bundle": "react-native bundle --entry-file index.js --bundle-output ios/baniking_mobile/main.jsbundle --dev false --platform ios --assets-dest ios/baniking_mobile",
    "ios-release": "npx react-native run-ios --configuration Release",
    "ios-debug": "npx react-native run-ios --configuration Debug"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/drawer": "^6.1.4",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "@types/mapbox__polyline": "^1.0.2",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-device-info": "^8.3.3",
    "react-native-dotenv": "^3.2.0",
    "react-native-fs": "^2.18.0",
    "react-native-geolocation-service": "^5.3.0-beta.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-global-props": "^1.1.5",
    "react-native-image-crop-picker": "^0.36.3",
    "react-native-image-resizer": "^1.4.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.29.3",
    "react-native-permissions": "^3.0.5",
    "react-native-reanimated": "^2.2.2",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-svg": "^12.1.1",
    "react-native-switch": "git+https://github.com/shahen94/react-native-switch.git",
    "react-native-text-input-mask": "^3.1.4",
    "react-native-tip": "^0.0.18",
    "react-native-vkontakte-login": "^1.0.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "rn-qr-generator": "^1.1.7",
    "rn-range-slider": "^2.1.1",
    "use-debounce": "^7.0.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-native": "^0.65.1",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-maps": "^0.24.2",
    "@types/react-native-text-input-mask": "^0.7.6",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-persist": "^4.3.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "typescript": "^4.4.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
